<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane fx:id="infoPane" prefHeight="655.0" prefWidth="923.0" styleClass="center-info-pane" stylesheets="@studentDashBoard.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mypack.project.StudentRegisteredCoursesController">
   <children>
      <ScrollPane prefHeight="655.0" prefWidth="923.0" style="-fx-background-color: #242529;">
        <content>
          <AnchorPane minHeight="655.0" minWidth="0.0" prefWidth="923.0" style="-fx-background-color: #242529;">
               <children>
                  <Pane fx:id="warning_box" layoutX="60.0" layoutY="24.0" prefHeight="50.0" prefWidth="739.0" style="-fx-background-color: #fff8ad; -fx-opacity: 1; -fx-background-radius: 15; -fx-border-color: yellow; -fx-border-radius: 15;">
                     <children>
                        <Label fx:id="warning_text" alignment="BASELINE_CENTER" layoutX="24.0" layoutY="17.0" text="Label">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </Pane>
                  <VBox fx:id="course_box" layoutX="60.0" layoutY="91.0" prefWidth="366.0">
                     <padding>
                        <Insets top="25.0" />
                     </padding>
                  </VBox>
                  <Button fx:id="resetbtn" layoutX="60.0" layoutY="29.0" mnemonicParsing="false" onAction="#resetRequest_clicked" prefHeight="40.0" prefWidth="123.0" styleClass="login-button" stylesheets="@studentDashBoard.css" text="Reset Request">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </children>
</Pane>
